<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="inquiry-Mapper">
	<resultMap type="Inquiry"
		id="inquiryResultSet">
		<result column="IN_NUM" property="inpuiryNum"></result>
		<result column="M_NUM" property="memberNum"></result>
		<result column="IN_TITLE" property="inpuiryTitle"></result>
		<result column="IN_CONTEXT" property="inpuiryContext"></result>
		<result column="IN_DATETIME_CREATED" property="inpuiryDateTimeCreated"></result>
		<result column="IN_ANSWER_FL" property="inpuiryAnswerFL"></result>
		<result column="IN_PUBLICE_FL" property="inpuiryPublicFL"></result>
	</resultMap>

	<resultMap type="InquiryAnswer"
		id="inquiryAnswerResultSet">
		<result column="IN_NUM" property="inpuiryNum"></result>
		<result column="IN_A_ADMIN" property="answerAdmin"></result>
		<result column="IN_A_TITLE" property="answerTitle"></result>
		<result column="IN_A_CONTEXT" property="answerContext"></result>
		<result column="IN_A_DATETIME_CREATED" property="answerDateTimeCreated"></result>
	</resultMap>

